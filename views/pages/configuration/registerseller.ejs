<link href='https://cdn.jsdelivr.net/npm/bulma-steps@2.2.1/dist/css/bulma-steps.min.css' rel='stylesheet'>
<script src="https://cdn.jsdelivr.net/npm/bulma-steps@2.2.1/dist/js/bulma-steps.js"></script>
<script src="https://cdn.jsdelivr.net/npm/marked/marked.min.js"></script>
<script src="https://sdk.mercadopago.com/js/v2"></script>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma-checkradio@1.1.1/dist/css/bulma-checkradio.min.css">
<link href='https://cdn.jsdelivr.net/npm/bulma-tooltip@3.0.2/dist/css/bulma-tooltip.min.css' rel='stylesheet'>
<div class="columns is-centered is-gapless">
  <div class="column is-8">
    <div class="container">
      <br><br>
      <div class="notification is-danger is-light is-hidden" id="error"></div>
      <div class="box">
        <div class="steps" id="stepsDemo">
          <div class="step-item is-info is-active" id="infobasics">
            <div class="step-marker">
              <span class="icon">
                <i class='bx bx-user' ></i>
              </span>
            </div>
            <div class="step-details">
              <p class="step-title">Información Básica</p>
            </div>
          </div>
          <div class="step-item is-info" id="address">
            <div class="step-marker">
              <span class="icon">
                <i class='bx bxs-map'></i>
              </span>
            </div>
            <div class="step-details">
              <p class="step-title">Dirección</p>
            </div>
          </div>
          <div class="step-item is-info" id="documents">
            <div class="step-marker">
              <span class="icon">
                <i class='bx bxs-file-doc'></i>
              </span>
            </div>
            <div class="step-details">
              <p class="step-title">Documentos</p>
            </div>
          </div>
          <div class="step-item is-info" id="suscriptions">
            <div class="step-marker">
              <span class="icon">
                <i class='bx bxs-credit-card'></i>
              </span>
            </div>
            <div class="step-details">
              <p class="step-title">Método de pago</p>
            </div>
          </div>
          <div class="step-item is-info" id="finish">
            <div class="step-marker">
              <span class="icon">
                <i class='bx bxs-check-shield'></i>
              </span>
            </div>
            <div class="step-details">
              <p class="step-title">Finalizar</p>
            </div>
          </div>
          <div class="steps-content">
            <div class="step-content has-text-centered is-active" id="contentinfo">
              <br>
              <form action="/registerseller" method="post" id='form-seller' enctype="multipart/form-data">
                <div class="columns">
                  <div class="column">
                    <div class="columns">
                      <div class="column">
                        <div class="field">
                          <label class="label is-size-7">Nombre del Comercio</label>
                          <div class="control">
                            <input class="input is-size-7" type="text" name="name" required="true" placeholder="Nombre del Seller" />
                          </div>
                        </div>
                      </div>
                      <div class="column">
                        <div class="field">
                          <label class="label is-size-7">Identificación</label>
                          <div class="control">
                            <input class="input is-size-7" type="text" name="dni" required="true" placeholder="NIT del Comercio" />
                          </div>
                        </div>
                      </div>
                      <div class="column">
                        <div class="field">
                          <label class="label is-size-7">Nombre del Contacto</label>
                          <div class="control">
                            <input class="input is-size-7" type="text" name="contact" required="true" placeholder="Nombre del contacto principal"/>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="columns">
                      <div class="column">
                        <div class="field">
                          <label class="label is-size-7">Email de Contacto</label>
                          <div class="control">
                            <input class="input is-size-7" type="text" name="email" required="true" placeholder="Email del contacto principal"/>
                          </div>
                        </div>
                      </div>
                      <div class="column">
                        <label class="label is-size-7">Contraseña</label>
                        <div class="field has-addons">
                          <p class="control has-icons-left is-expanded">
                            <input class="input pass is-size-7" name="password" type="password" minlength="6" required placeholder="Contraseña Ver / Ocultar" autocomplete="no-fill">
                            <span class="icon is-size-7 is-left">
                              <i class='bx bxs-lock-alt'></i>
                            </span>
                          </p>
                          <p class="control" id="ojo">
                            <a class="button is-size-7" id="eye">
                              <i class='bx bx-low-vision'></i>
                            </a>
                          </p>
                        </div>
                      </div>
                      <div class="column">
                        <label class="label is-size-7">Número Celular</label>
                        <div class="field has-addons">
                          <p class="control">
                          <span class="select is-size-7">
                            <select required name="country">
                            <% countries.forEach((country) =>{ %>
                              <% if(country.active){ %>
                              <option class="is-uppercase" value="<%=country.id%>">+<%=country.prefix%></option>
                              <%}%>
                            <% })%>
                            </select>
                          </span>
                          </p>
                          <p class="control has-icons-left is-expanded">
                            <input class="input is-size-7" required name="phone" type="number" placeholder="Número Móvil">
                            <span class="icon is-small is-left is-size-7">
                              <i class='bx bx-mobile'></i>
                            </span>
                          </p>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="columns">
                  <div class="column">
                    <div class="field">
                      <label class="label is-size-7">Logotipo</label>
                      <div id="file-loader" class="file is-size-7 has-name is-fullwidth">
                        <label class="file-label">
                          <input class="file-input" accept="image/*" type="file" id="logo" name="logo" />
                          <span class="file-cta">
                            <span class="file-icon">
                              <i class="bx bx-upload"></i>
                            </span>
                            <span class="file-label">
                              Logotipo 281*63
                            </span>
                          </span>
                          <span class="file-name" id="spanlogo">
                            No ha cargado ningún archivo.
                          </span>
                        </label>
                      </div>
                    </div>
                  </div>
                  <div class="column">
                    <div class="field">
                      <label class="label is-size-7">Moneda</label>
                      <div class="control">
                        <div class="select is-fullwidth is-size-7">
                          <select required class="select is-capitalized" name="currency">
                            <option value="">Elegir Moneda</option>
                            <% currencies.forEach((currency) =>{ %>
                              <option class="is-capitalized" value="<%=currency.id%>"><%=currency.name%></option>
                            <%})%>
                          </select>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="columns is-mobile">
                  <div class="column is-three-fifths is-offset-one-fifth">
                    <div class="field">
                      <input class="is-checkradio is-small is-info" required id="checkboxterms" type="checkbox" name="checkboxterms">
                      <label for="checkboxterms"><strong>Acepto los <a href="#" id="accept-terms">Términos y Condiciones</a></strong></label>
                    </div>
                  </div>
                </div>
              </form>
            </div>
            <div class="step-content has-text-centered" id="contentaddress">
              <br>
              <form method="POST" action="/setaddress" id="form-address">
                <div class="columns">
                  <div class="column">
                    <div class="field">
                      <label class="label is-size-7">Dirección (Línea 1)</label>
                      <div class="control">
                        <input class="input is-size-7" required name="addressline1" type="text" placeholder="Calle, Carrera, Avenida, etc..." />
                      </div>
                    </div>
                  </div>
                  <div class="column">
                    <div class="field">
                      <label class="label is-size-7">Dirección (Línea 2)</label>
                      <div class="control">
                        <input class="input is-size-7" name="addressline2" type="text" placeholder="Torre, Casa, Piso, Puerta, Edificio..." />
                      </div>
                    </div>
                  </div>
                </div>
                <div class="columns">
                  <div class="column">
                    <div class="field">
                      <label class="is-size-7 label">País
                        <span class="icon is-small has-text-info has-tooltip-top has-tooltip-info" data-tooltip="Debes seleccionar el país">
                          <i class='bx bxs-info-circle'></i>
                        </span>
                      </label>
                      <div class="control">
                        <div class="select is-fullwidth is-size-7">
                          <select required class="select is-capitalized" name="country">
                            <option value="">-- Elegir País --</option>
                            <% countries.forEach((country) =>{ %>
                              <option class="is-capitalized" value="<%=country.id%>"><%=country.name%></option>
                            <%})%>
                          </select>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="column">
                    <div class="field" id="estados">
                      <label class="label is-size-7">Estado</label>
                      <div class="select control is-size-7 is-fullwidth">
                        <select class="is-capitalized" name="region" required >
                          <option value="">-- Elegir Región --</option>
                        </select>
                      </div>
                    </div>
                  </div>
                  <div class="column">
                    <div class="field" id="ciudades">
                      <label class="label is-size-7">Ciudad</label>
                      <div class="control select is-size-7 is-fullwidth">
                        <select class="is-capitalized" name="city" required>
                          <option value="">-- Elegir Ciudad --</option>
                        </select>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="columns">
                  <div class="column">
                    <div class="field">
                      <label class="label is-size-7">Código Postal</label>
                      <div class="control">
                        <input class="input is-size-7" name="zipcode" type="text" placeholder="Código Postal de la Dirección" />
                      </div>
                    </div>
                  </div>
                  <div class="column">
                    <div class="field">
                      <label class="label is-size-7">Detalles adicionales para ubicar la dirección</label>
                      <div class="control">
                        <textarea class="textarea is-size-7" name="notes" placeholder="Detalles adicionales que puedan ser de utilidad para localizar esta dirección."></textarea>
                      </div>
                    </div>
                  </div>
                </div>
              </form>
            </div>
            <div class="step-content has-text-centered" id="contentdocuments">
              <article class="message is-info is-small">
                <div class="message-header">
                  <p>Información</p>
                </div>
                <div class="message-body">
                  Adjunte tus documentos legales en pdf con los siguientes nombres <strong>RUT, Cámara de comercio, Cédula representante legal.</strong>
                </div>
              </article>
              <div class="columns">
                <div class="column is-centered">
                  <input type="hidden" name="seller-data" id="seller-data"/>
                  <div class="file is-boxed">
                    <label class="file-label">
                      <input class="file-input" type="file" name="document" multiple accept="application/pdf" />
                      <span class="file-cta">
                        <span class="file-icon">
                          <i class='bx bxs-cloud-upload bx-sm'></i>
                        </span>
                        <span class="file-label">
                          Escoge tus archivos…
                        </span>
                      </span>
                    </label>
                  </div>
                </div>
                <div class="column is-two-thirds">
                  <progress class="progress is-small is-primary is-hidden" max="100">15%</progress>
                  <div class="preview">
                  </div>
                </div>
              </div>
            </div>
            <div class="step-content has-text-centered" id="contentsuscriptions">
              <article class="message is-warning is-small">
                <div class="message-header">
                  <p>Información</p>
                </div>
                <div class="message-body">
                  Al momento de guardar tu método de pago <strong>se descontará una tarifa por el registro de tu cuenta.</strong>
                </div>
              </article>
              <form method="POST" action="/createcreditcard" id="form-suscription">
                <div class="is-mediumwidth container-card">
                  <p class="has-text-centered">
                    <img src="/images/paymentlogos.webp">
                  </p>
                  <div class="content is-size-7">
                    <form action="/createcreditcard" method="POST">
                      <div class="field">
                        <div class="control has-icons-right">
                          <input class="input is-small" required type="number" name="card" placeholder="Número de la Tarjeta"/>
                          <span class="icon is-right">
                            <i class='bx bx-credit-card-front'></i>
                          </span>
                        </div>
                      </div>
                      <div class="columns is-mobile">
                        <div class="column is-5">
                          <label class="label is-small" for="month">Fecha vencimiento</label>
                          <div class="control is-inline-block">
                            <div class="select is-small">
                              <select name="month" required>
                                <%for(let i=1; i<=12; i++){%>
                                  <%if(i<10){%>
                                    <option value="0<%=i%>">0<%=i%></option>
                                  <%}else{%>
                                    <option value="<%=i%>"><%=i%></option>
                                  <%}%>
                                <%}%>
                              </select>
                            </div>
                          </div>
                          <div class="control is-inline-block">
                            <div class="select is-small">
                              <% let year = (new Date()).getFullYear() %>
                              <select name="year" required>
                                <%for(let i=year; i<=year+8; i++){%>
                                  <option value="<%=i%>"><%=i%></option>
                                <%}%>
                              </select>
                            </div>
                          </div>
                        </div>
                        <div class="column is-4">
                          <label class="label is-small" for="cvv">Cod. seguridad</label>
                          <div class="field">
                            <div class="control">
                              <input class="input is-small" required type="number" name="cvv" placeholder="CVV / CVC"/>
                            </div>
                          </div>
                        </div>
                      </div>
                      <div class="field">
                        <div class="control has-icons-right">
                          <input class="input is-small" required type="text" name="cardname" placeholder="Nombre cómo aparece en la tarjeta"/>
                          <span class="icon is-right">
                            <i class='bx bxs-user-rectangle' ></i>
                          </span>
                        </div>
                      </div>
                      <div class="columns is-mobile">
                        <div class="column is-two-fifths">
                          <div class="field">
                            <div class="control">
                              <div class="select is-small">
                                <select name="tid" required>
                                  <option value="CC">CC</option>
                                  <option value="CE">CE</option>
                                  <option value="NIT">NIT</option>
                                  <option value="PPN">PPN</option>
                                  <option value="SSN">SSN</option>
                                  <option value="LIC">LIC</option>
                                  <option value="DNI">DNI</option>
                                  <option value="RFC">RFC</option>
                                </select>
                              </div>
                            </div>
                          </div>
                        </div>
                        <div class="column is-three-fifths">
                          <div class="field">
                            <div class="control has-icons-right">
                              <input class="input is-small" required type="number" name="dni" placeholder="Número de identificación"/>
                              <span class="icon is-right">
                                <i class='bx bx-id-card'></i>
                              </span>
                            </div>
                          </div>
                        </div>
                      </div>
                    </form>
                  </div>
                </div>
              </form>
            </div>
            <div class="step-content has-text-centered" id="contentfinish">
              <article class="message is-info" id="msg-process">
                <div class="message-body">
                  <p>Por favor espera, estamos procesando tu pago. Tomará unos minutos.</p><br>
                  <progress class="progress is-small is-primary" max="100">15%</progress>
                </div>
              </article>
              <article class="message is-success is-hidden" id="msg-success">
                <div class="message-body">
                  ¡Tu cuenta se ha creado correctamente!
                </div>
              </article>
            </div>
          </div>
          <div class="steps-actions">
            <div class="steps-action">
              <a href="#" id="btn-back" data-nav="previous" class="button is-primary is-small">Atrás</a>
            </div>
            <div class="steps-action">
              <a href="#" id="btn-next" data-nav="next" class="button is-primary is-small">Siguiente</a>
            </div>
          </div>
        </div>
      </div>
      <div id="modal-terms"  class="modal">
        <div class="modal-background"></div>
        <div class="modal-card modal-terms">
          <section class="modal-card-body">
            <div class="content" id="content"></div>
          </section>
        </div>
        <button class="modal-close is-large" aria-label="close"></button>
      </div>
    </div>
  </div>
</div>
<script>
  bulmaSteps.attach();
  let sellerId = '';
  let userId = '';
  let fmanager = document.querySelector('input[name="document"]');
  fmanager.onchange = createDocuments;
  const fileInput = document.querySelector('#file-loader input[type=file]');
  fileInput.onchange = () => {
    if (fileInput.files.length > 0) {
      const fileName = document.querySelector('#file-loader .file-name');
      fileName.textContent = fileInput.files[0].name;
    }
  };

  live('#logo','change', e =>{
    const input = document.querySelector('#logo');
    const error = document.querySelector('#error');
    let url = window.URL || window.webkitURL;
    let image, file;
    if ((file = input.files[0])) {
      image = new Image();
      image.onload = function() {
        if (this.width > 281 && this.height > 63) {
          input.value = '';
          document.querySelector('#spanlogo').innerHTML = 'No ha cargado ningún archivo.';
          error.innerHTML = 'El logo debe tener un tamaño de 281*63';
          removeClass(error, 'is-hidden');
          setTimeout(function() {
            addClass(error,'is-hidden');
          }, 5000);
        }
      };
      image.src = url.createObjectURL(file);
    }
  });

  live('#eye','click',e =>{
    let pass = document.querySelector('.pass');
    if(pass.type==='password'){
      pass.type='text';
      addClass(document.querySelector('#ojo > .button'),'is-danger');
    }else{
      pass.type='password';
      removeClass(document.querySelector('#ojo > .button'),'is-danger');
    }
  });

  live('select[name="country"]','change',e =>{
    let country = e.target.value;
    
    let regions = document.querySelector('#estados')
    regions.querySelector('select').innerHTML='<option value="">-- Elegir Región --</option>';
    addClass(regions,'is-hidden');
    
    let cities = document.querySelector('#ciudades')
    cities.querySelector('select').innerHTML='<option value="">-- Elegir Ciudad --</option>';
    addClass(cities,'is-hidden');
    
    io.socket.get('/find/regions/'+country,resdata =>{
      if(resdata.regions.length>0){
        for(let s of resdata.regions){
          let option = document.createElement('option');
          option.value = s.id;
          option.innerHTML = s.name
          regions.querySelector('select').appendChild(option);
        }
        removeClass(regions,'is-hidden');
      }
    });
  });

  live('select[name="region"]','change',e =>{
    let region = e.target.value;
    let cities = document.querySelector('#ciudades')
    cities.querySelector('select').innerHTML='<option value="">-- Elegir Ciudad --</option>';
    addClass(cities,'is-hidden');
    io.socket.get('/find/cities/'+region,resdata =>{
      if(resdata.cities.length>0){
        for(let s of resdata.cities){
          let option = document.createElement('option');
          option.value = s.id;
          option.innerHTML = s.name
          cities.querySelector('select').appendChild(option);
        }
        removeClass(cities,'is-hidden');
      }
    });
  });

  live('#btn-next','click',function(e){
    e.preventDefault();
    const stepNext = document.querySelector('.step-item.is-info.is-active');
    const stepId = stepNext.getAttribute('data-step-id') - 1;
    if (stepId === 0) {
      createSeller(stepNext);
    } else if(stepId === 1) {
      createAddress(stepNext);
    } else if(stepId === 2) {
      let numChildren = document.querySelector('.preview').children.length;
      if (numChildren < 3) {
        showError('Debe subir los documentos requeridos', stepNext, document.querySelector('#documents'), document.querySelector('#contentsuscriptions'), document.querySelector('#contentdocuments'));
      }
    } else if (stepId === 3) {
      let form = document.querySelector('#form-suscription');
      let validate = validateForm(form);
      if (validate && sellerId) {
        createCreditCard(stepNext, form);
      } else {
        showError('Complete los campos requeridos', stepNext, document.querySelector('#suscriptions'), document.querySelector('#contentfinish'), document.querySelector('#contentsuscriptions'));
        document.querySelector('#btn-next').removeAttribute('disabled');
      }
    }
  });

  live('.delete','click',function(e){
    let docId = this.getAttribute('docId');
    let art = document.querySelector(`#article${docId}`)
    io.socket.delete(`/document/remove/${docId}`,function(data, Jwres){
      if(data === 'ok'){
        art.remove();
      }
    });
  });

  live('#accept-terms','click',function(e){
    e.preventDefault();
    let form = document.querySelector('#form-seller');
    let modal = document.querySelector('#modal-terms');
    let section = document.querySelector('#content');
    let currency = form.querySelector('select[name="currency"]').value
    const currencies = <%- JSON.stringify(currencies) %>;
    currency = currencies.find(c => c.id === currency);
    if (currency && currency.isocode === 'MXN') {
      showTermsAndConditionsMX(section);
    } else {
      showTermsAndConditionsCO(section);
    }
    addClass(modal,'is-active');
  });

  live('.modal-background, .delete, .modal-close','click',e=>{
    let modals = document.querySelectorAll('.modal');
    modals.forEach(modal => {
      removeClass(modal,'is-active');
    });
  });

  live('.is-checkradio','click', e=>{
    let element = e.target;
    if (hasClass(element,'has-background-color')) {
      removeClass(element,'has-background-color');
    } else {
      addClass(element,'has-background-color');
    }
  });

  function createSeller(stepNext) {
    let form = document.querySelector('#form-seller');
    let validate = validateForm(form);
    const terms = form.querySelector('input[name="checkboxterms"]').checked;
    if(validate){
      if (terms) {
        let request = new XMLHttpRequest();
        let data = new FormData(form);
        data.append('seller', sellerId);
        data.append('user', userId);
        data.append('terms', terms);
        request.addEventListener('load', function(e) {
          const resData = JSON.parse(request.response);
          if (request.status == 200) {
            if (resData && resData.error) {
              showError(resData.error, stepNext, document.querySelector('#infobasics'), document.querySelector('#contentaddress'), document.querySelector('#contentinfo'));
              document.querySelector('#btn-back').setAttribute('disabled', 'disabled');
            } else {
              sellerId = resData.seller;
              userId = resData.user;
            }
          } else {
            showError(resData.error, stepNext, document.querySelector('#infobasics'), document.querySelector('#contentaddress'), document.querySelector('#contentinfo'));
            document.querySelector('#btn-back').setAttribute('disabled', 'disabled');
          }
        });
        request.open('POST', form.action, true);
        request.send(data);
      } else {
        showError('Acepte los Términos y Condiciones', stepNext, document.querySelector('#infobasics'), document.querySelector('#contentaddress'), document.querySelector('#contentinfo'));
        document.querySelector('#btn-back').setAttribute('disabled', 'disabled');
      }
    } else {
      showError('Complete los campos requeridos', stepNext, document.querySelector('#infobasics'), document.querySelector('#contentaddress'), document.querySelector('#contentinfo'));
      document.querySelector('#btn-back').setAttribute('disabled', 'disabled');
    }
  }

  function createAddress(stepNext) {
    let form = document.querySelector('#form-address');
    let validate = validateForm(form);
    if (validate && sellerId) {
      io.socket.post(`${form.action}/${sellerId}`, {
        type: 'register',
        addressline1: form.querySelector('input[name="addressline1"]').value,
